private int[] sortFaces(Triangle3D[]t){  float[]zStack = new float[t.length];  int[]sortOrder = new int[t.length];    for (int i=0; i< t.length; i++){      zStack[i] = (t[i].v[0].z + t[i].v[1].z + t[i].v[2].z)/3;      sortOrder[i] = i;    }    for (int i=0; i<t.length; i++) {      for (int j=1; j<t.length-i; j++) {        if (zStack[j-1] > zStack[j]) {          float zTemp =  zStack[j-1];          zStack[j-1] = zStack[j];          zStack[j] = zTemp;          int orderTemp = sortOrder[j-1];          sortOrder[j-1] = sortOrder[j];          sortOrder[j] = orderTemp;        }      }    }    return sortOrder;  }