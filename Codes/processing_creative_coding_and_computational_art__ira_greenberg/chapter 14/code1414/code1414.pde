public class MyController extends PApplet {  Cube c;   Triangle3D[]t;  void setup(){    size(400, 400, P3D);    c = new Cube(175, 175, 175);    t = c.getTriangles();    noStroke();  }  void draw(){    background(100);    lights();    fill(150);    translate(width/2, height/2);    rotateY(frameCount*PI/100);    rotateX(frameCount*PI/75);    for (int i=0; i<t.length; i++){      beginShape(TRIANGLES);      for (int j=0; j<3; j++){        vertex(t[i].v[j].x, t[i].v[j].y, t[i].v[j].z);      }      endShape();    }  }}