/* Poly Maker Ira Greenberg, November 26, 2005 revised October 18, 2006 */void setup(){  this.size(400, 400);  this.background(0);  this.smooth();  this.noFill();  this.makePoly(width/2, height/2, 9, 150, 255, 8, MITER);}void makePoly(int x, int y, int points, float radius,         int strokeCol, float strokeWt, int strokeJn){  float px=0, py=0;  float angle = 0;  this.stroke(strokeCol);  this.strokeJoin(strokeJn);  this.strokeWeight(strokeWt);  this.beginShape();  for (int i = 0; i< points; i++){    px = x+ this.cos(this.radians(angle))*radius;    py = y+ this.sin(this.radians(angle))*radius;    this.vertex(px, py);    angle+=360/points;  }   this.endShape(CLOSE);}