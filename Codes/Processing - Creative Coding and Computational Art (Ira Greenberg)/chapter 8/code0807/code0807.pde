// polymorphism example using inheritancevoid setup(){  Creature c = new Creature();  Ogre o = new Ogre();  Elf  e = new Elf();  Engine eng = new Engine();  eng.addCreature(c);  eng.addCreature(o);  eng.addCreature(e);}//superclassclass Creature{  void castSpell(){    println("nothing to say");  }}//subclass 1class Ogre extends Creature{  void castSpell(){    println("I miss ya donkey");  }}//subclass 2class Elf extends Creature{  void castSpell(){    println("Gotta make the cookies");  }}/* game engine-with embedded superclass  reference in the addCreature() method */class Engine{  void addCreature(Creature c){    c.castSpell();  }}