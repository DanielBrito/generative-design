color rectBackground = color(0, 0, 255);void setup(){  size(400, 400);  background(255);  rectMode(CENTER);}void draw(){  fill(rectBackground);  rect(width/2, height/2, 200, 200);  // keyPressed boolean variable  if (keyPressed){     if(key == 'a'){      rectBackground = color(255, 0, 0);    }     else if (key == 's') {      rectBackground = color(255, 255, 0);    }     else if (key == 'w') {      rectBackground = color(0, 255, 0);    }     else if (key == 'z') {      rectBackground = color(255, 0, 255);    }  }}