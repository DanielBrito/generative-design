color rectBackground = color(0, 0, 255);void setup(){  size(400, 400);  background(255);  rectMode(CENTER);}void draw(){  fill(rectBackground);  rect(width/2, height/2, 200, 200);}void keyPressed(){   if(key == CODED){    if (keyCode == UP) {      rectBackground = color(255, 0, 0);    }     else if (keyCode == DOWN) {      rectBackground = color(255, 255, 0);    }     else if (keyCode == RIGHT) {      rectBackground = color(0, 255, 0);    }     else {      rectBackground = color(255, 0, 255);    }  }}