public class MyController extends PApplet {  IG3D i;  Cube c;  void setup(){    size(400, 400);    fill(127, 255, 0);    noStroke();    i = new IG3D(this);    c = new Cube(150, 150, 150);  }  void draw(){    background(0);    i.translateXYZ(width/2, height/2, -200);    i.setViewAngle(70);    i.rotX(frameCount*3);    i.rotY(frameCount*2);    i.rotZ(frameCount*1);    i.setViewAngle(75);    i.render(c);  }}