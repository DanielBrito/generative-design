import processing.core.PApplet;public class Vector3D{  public float x, y, z;  private float[]origVals;  public Vector3D(){  }  public Vector3D(float x, float y, float z){    this.x = x;    this.y = y;    this.z = z;    // capture original values    origVals = new float[]{x, y, z};  }   //methods  public void add(Vector3D v){    x+=v.x;    y+=v.y;    z+=v.z;  }  public void subtract(Vector3D v){    x-=v.x;    y-=v.y;    z-=v.z;  }  public void multiply(float s){    x*=s;    y*=s;    z*=s;  }  public void divide(float s){    x/=s;    y/=s;    z/=s;  } public void reset(){    x = origVals[0];    y = origVals[1];    z = origVals[2];  }public void setTo(Vector3D v){  x = v.x;  y = v.y;  z = v.z;    }public float getMagnitude(){    return PApplet.sqrt(x*x+y*y+z*z);  }}