public Vector3D getCrossProduct(Vector3D v1, Vector3D v2){    v1 = new Vector3D(v1.x-x, v1.y-y, v1.z-z);    v2 = new Vector3D(v2.x-x, v2.y-y, v2.z-z);    Vector3D cp = new Vector3D();    // actual cross product calculation    cp.x = v1.y*v2.z - v1.z*v2.y;    cp.y = v1.z*v2.x - v1.x*v2.z;    cp.z = v1.x*v2.y - v1.y*v2.x;    // normalize    cp.divide(cp.getMagnitude());    return cp;  }