public class MyController extends PApplet {  IG3D i;  Rectangle3D r;  void setup(){    size(400, 400);    fill(127, 255, 0);    i = new IG3D(this);    r = new Rectangle3D(250, 200);  }  void draw(){    background(0);    i.translateXYZ(width/2, height/2, -100);    i.setViewAngle(70);     i.rotY(frameCount*2);    stroke(0);    i.render(r);    stroke(255);    i.renderNormals(r, 100);  }}